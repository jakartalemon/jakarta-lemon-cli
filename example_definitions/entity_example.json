{
    "storageType": "h2",
    "connectionType": "DataSourceEmbedded",
    "connectionInfo": {
        "url": "jdbc:h2:mem:testdb",
        "user": "sa",
        "password": "",
        "properties": {}
    },
    "entities": [
        {
            "AddressEntity": {
                "fields": {
                    "addressId": {
                        "primaryKey": true,
                        "type": "String",
                        "length": 10
                    } 
                }  
            },
            "PersonEntity": {
                "mapToModel": "Person",
                "fields": {
                    "personId": {
                        "primaryKey": true,
                        "type": "String",
                        "length": 10,
                        "name": "primary_key"
                    },
                    "firstName": {
                        "type": "String" 
                    },
                    "address": {
                        "type": "AddressEntity",
                        "oneToOne": true,
                        "name": "address_id"  
                    }
                }
            },
            "BankEntity": {
                "mapToModel": "Bank",
                "fields": {
                    "bankId": {
                        "primaryKey": true,
                        "type": "String",
                        "length": 30,
                        "name": "bank_id",
                        "strategyId": "UUID"
                    },
                    "name": {
                        "type": "String",
                        "length": 100
                    }
                }
            },
            "LocalBankEntity": {
                "fields": {
                    "localBankId": {
                        "primaryKey": true,
                        "type": "String",
                        "length": 30,
                        "name": "local_bank_id",
                        "strategyId": "UUID"
                    },
                    "name": {
                        "type": "String",
                        "length": 100
                    },
                    "bank": {
                        "type": "BankEntity",
                        "manyToOne": true,
                        "name": "bank_id"
                    }
                }
            }
        }
    ]
}